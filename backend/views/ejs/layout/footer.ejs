<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
    />
  </head>
  <body>
    <!-- Footer -->
    <footer
      class="text-center text-lg-start text-dark"
      style="background-color: #21d192"
    >
      <!-- Section: Links  -->
      <section class="">
        <div class="container text-center text-md-start mt-5">
          <!-- Remove the container if we want to extend the Footer to full width. container/container-fluid -->
          <!-- Grid row -->
          <div class="row mt-3">
            <!-- Grid column 1 -->
            <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
              <!-- Content -->
              <h6 class="text-uppercase fw-bold">Nimap Infotech</h6>
              <hr
                class="mb-4 mt-0 d-inline-block mx-auto"
                style="width: 60px; background-color: #7c4dff; height: 2px"
              />
              <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
            </div>
            <!-- End of Grid column 1 -->

            <!-- Grid column 2 -->
            <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">PRODUCT</h6>
              <hr
                class="mb-4 mt-0 d-inline-block mx-auto"
                style="width: 60px; background-color: #7c4dff; height: 2px"
              />
              <p>
                <a href="/addProduct" class="text-dark">Add Product</a>
              </p>
              <p>
                <a href="/getProducts" class="text-dark">Get Products</a>
              </p>
              <p>
                <a href="/getAllProducts" class="text-dark">Get All Products</a>
              </p>
            </div>
            <!-- End of Grid column 2 -->

            <!-- Grid column 3 -->
            <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">CATEGORY</h6>
              <hr
                class="mb-4 mt-0 d-inline-block mx-auto"
                style="width: 60px; background-color: #7c4dff; height: 2px"
              />
              <p>
                <a href="/addCategory" class="text-dark">Add Category</a>
              </p>
              <p>
                <a href="/getCategory" class="text-dark">Get Category</a>
              </p>
            </div>
            <!-- End of Grid column 3 -->

            <!-- Grid column 4 -->
            <div class="col-md-4 col-lg-3 col-xl-3 mx-auto mb-md-0 mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">Contact</h6>
              <hr
                class="mb-4 mt-0 d-inline-block mx-auto"
                style="width: 60px; background-color: #7c4dff; height: 2px"
              />
              <p><i class="fas fa-home mr-3"></i> Mumbai</p>
              <p><i class="fas fa-envelope mr-3"></i> user@example.com</p>
              <p><i class="fas fa-phone mr-3"></i> + 01 234 567 88</p>
            </div>
            <!-- End of Grid column 4 -->
          </div>
          <!-- End of Grid row -->
        </div>
      </section>
      <!-- End of Section: Links  -->

      <!-- Section: Social media -->
      <section
        class="d-flex justify-content-between p-4 text-white"
        style="background-color: #21d192"
